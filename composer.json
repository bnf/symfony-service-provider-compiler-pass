{
    "name": "bnf/symfony-service-provider-compiler-pass",
    "description": "Symfony compiler pass that allows to use service providers as defined in container-interop/service-provider",
    "keywords": ["PSR-11", "Symfony", "container-interop", "service-provider"],
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Franzke"
        },
        {
            "name": "David NÃ©grier"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "symfony/dependency-injection": "^4.1",
        "container-interop/service-provider": "~0.4.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || ^7.0",
        "php-coveralls/php-coveralls": "^2.0",
        "bnf/di": "^0.1.2"
    },
    "autoload": {
        "psr-4": { "Bnf\\SymfonyServiceProviderCompilerPass\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Bnf\\SymfonyServiceProviderCompilerPass\\Tests\\": "tests/" }
    }
}
